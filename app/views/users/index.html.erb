<% provide(:title, 'All members') %>
<h1>All members</h1>

<%= form_tag import_users_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= render @users %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
